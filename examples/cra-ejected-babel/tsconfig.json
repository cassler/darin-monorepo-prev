{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es2015",
			"es6",
			"es7",
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"baseUrl": "./",
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react"
	},
	"include": [
		"src"
	]
}
