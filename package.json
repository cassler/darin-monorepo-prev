{
  "name": "workflower-monorepo",
  "version": "1.1.0",
  "description": "A monorepo for Workflower using lerna, typescript, yarn workspaces and typescript",
  "main": "index.js",
  "author": "darin@cassler.net",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "start": "lerna run start --parallel",
    "start:dev": "lerna run start:dev --parallel",
    "test": "lerna run test --parallel",
    "test:e2e": "lerna run test:e2e --parallel",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "barrels": "lerna run generate-barrels --parallel",
    "webserver": "yarn workspace @wf/react-ts light-server -s . -p 7000",
    "docs": "yarn workspace @wf/docs run start",
    "react": "yarn workspace @wf/react-ts run start",
    "deploy-docs": "yarn workspace @wf/docs run ghe-deploy"
  },
  "dependencies": {
    "lerna": "^3.22.1"
  }
}
