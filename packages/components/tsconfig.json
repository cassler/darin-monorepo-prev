{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"target": "es5",
		"allowJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "react",
		"incremental": true,
		"lib": [
			"dom",
			"dom.iterable",
			"esnext",
		],
	},
	"include": [
		"src"
	]
}
